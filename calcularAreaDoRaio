import java.util.Scanner;

class Ex2 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
			double raio,area, pi = 1.14159;
			
			raio = sc.nextDouble();
			
			area = pi * raio * raio;
			
			System.out.printf("A area Ã© de %.4f%n", area);
			
			
			sc.close();	
			

	}

}
